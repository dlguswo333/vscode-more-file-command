{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "out",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"strictNullChecks": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
		"paths": {
			"@/*": ["./src/*"]
		}
	},
}
